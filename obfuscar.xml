<?xml version="1.0" encoding="utf-8"?>
<Obfuscator>
  <Var name="InPath" value="." />
  <Var name="OutPath" value="Confused" />
  <Var name="KeepPublicApi" value="true" />
  <Var name="HidePrivateApi" value="true" />
  <Var name="RenameProperties" value="false" />
  <Var name="RenameEvents" value="false" />
  <Var name="RenameFields" value="false" />
  <Var name="RenameMethods" value="false" />
  <Var name="UseUnicodeNames" value="false" />
  <Var name="UseKoreanNames" value="false" />
  <Var name="HideStrings" value="false" />
  <Var name="OptimizeMethods" value="true" />
  
  <Module file="$(InPath)\Canvas Cast.dll">
    <!-- 跳过资源类 -->
    <SkipType name="*.Resources" />
    <SkipType name="*.Properties.*" />
    
    <!-- 跳过 XAML 相关类（WPF 必需） -->
    <SkipType name="*Window" />
    <SkipType name="*Page" />
    <SkipType name="*UserControl" />
    
    <!-- 跳过 App 入口 -->
    <SkipType name="*.App" />
    
    <!-- 跳过数据库实体类（EF Core） -->
    <SkipNamespace name="*.Database.Models*" />
    
    <!-- 跳过 DTO 类（用于序列化） -->
    <SkipNamespace name="*.DTOs*" />
    
    <!-- 跳过枚举类型 -->
    <SkipNamespace name="*.Enums*" />
    
    <!-- 跳过 ViewModel（可能被 XAML 绑定） -->
    <SkipNamespace name="*.ViewModels*" />
    
    <!-- 跳过 Services（依赖注入和接口实现） -->
    <SkipNamespace name="*.Services*" />
    
    <!-- 跳过 Repositories（数据库操作） -->
    <SkipNamespace name="*.Repositories*" />
    
    <!-- 跳过 Managers（业务逻辑管理器） -->
    <SkipNamespace name="*.Managers*" />
  </Module>
</Obfuscator>
